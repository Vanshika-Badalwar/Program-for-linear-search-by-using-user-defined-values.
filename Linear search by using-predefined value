#include <iostream>
using namespace std;

int main()
{
    int arr[5] = {10, 25, 30, 45, 50};
    int key = 30;
    int i, found = 0;

    for(i = 0; i < 5; i++)
    {
        if(arr[i] == key)
        {
            cout("Element found at position " i + 1);
            found = 1;
            break;
        }
    }

    if(found == 0)
    {
        cout("Element not found");
    }

    return 0;
}
