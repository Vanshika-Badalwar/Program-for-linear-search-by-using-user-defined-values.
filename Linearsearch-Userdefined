#include <iostream>
using namespace std;

int main()
{
    int arr[50];
    int key;
    bool found = false;

    cout << "Enter 50 elements:\n";
    for (int i = 0; i < 50; i++)
    {
        cin >> arr[i];
    }

    cout << "Enter element to search: ";
    cin >> key;

    for (int i = 0; i < 50; i++)
    {
        if (arr[i] == key)
        {
            cout << "Element found at position: " << i + 1;
            found = true;
            break;
        }
    }

    if (!found)
    {
        cout << "Element not found";
    }

    return 0;
}
